{
  "manifest_version": 2,

  "name": "GIF Tab",
  "description": "See a collection of trending GIFs every time you open a new tab!",
  "version": "1.2",
  "icons": { "16": "icon-16.png",
            "48": "icon-48.png",
            "128": "icon-128.png" },
  "offline_enabled": true,

  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}