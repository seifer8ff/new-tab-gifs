{
  "manifest_version": 2,
  "name": "GIF Tab",
  "description": "See a collection of trending GIFs every time you open a new tab!",
  "version": "1.3",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "offline_enabled": true,
  "browser_action": {
    "default_icon": {
      "16": "img/icon-16.png",
      "48": "img/icon-48.png",
      "128": "img/icon-128.png"
    },
    "default_title": "GIF Tab",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}